{% extends 'base.html.twig' %}

{% block title %}
	{{ parent() }}
	- Saison
{% endblock %}

{% block body %}
	<h1>Saison</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Série</th>
				<td>{{ season.program.title }}</td>
			</tr>
			<tr>
				<th>Id</th>
				<td>{{ season.id }}</td>
			</tr>
			<tr>
				<th>Numéro</th>
				<td>{{ season.number }}</td>
			</tr>
			<tr>
				<th>Année</th>
				<td>{{ season.year }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ season.description }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_season_index') }}">Index des saisons</a>

	<a href="{{ path('app_season_edit', {'id': season.id}) }}">Modifier</a>

	{{ include('season/_delete_form.html.twig') }}
{% endblock %}
