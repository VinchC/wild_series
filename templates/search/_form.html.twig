{{ form_start(form) }}
<div class="errors">
	{{ form_errors(form) }}
</div>
<fieldset>
	{{ form_errors(form.search)}}
	{{ form_label(form.search)}}
	{{ form_widget(form.search)}}
	{{ form_help(form.search)}}
</fieldset>
<button type="submit">{{ button_label|default('Rechercher') }}</button>
{{ form_end(form) }}
